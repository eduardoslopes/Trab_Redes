package com.piratedropbox.model;

//TAG      |        UC
//---------|---------------------------------------|
//INSERTAP | Inserir um arquivo em uma pasta       |
//INSERTPP | Inserir uma pasta em uma pasta        |
//CREATEP  | Criar uma pasta                       |
//SEEAP    | Ver arquivos e pastas de uma pasta    |
//DOWNA    | Baixar um arquivo                     |
//SHAREA   | Compartilhar um arquivo               |
//SHAREP   | Compartilhar uma pasta                |

public class TAG {
	public static final String INSERTAP = "INSERTAP";
	public static final String INSERTPP = "INSERTPP";
	public static final String CREATEP = "CREATEP";
	public static final String SEEP = "SEEP";
	public static final String DOWNA = "DOWNA";
	public static final String SHAREA = "SHAREA";
	public static final String SHAREP = "SHAREP";
	public static final String CREATEU = "CREATEU";
	public static final String LOGINU = "LOGINU";
	public static final String SUCCESS = "SUCCESS";
	public static final String FAIL = "FAIL";
}
